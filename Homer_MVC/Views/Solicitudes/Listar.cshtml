@{
    Layout = "~/Views/Shared/_Layout_null.cshtml";
    var desde = ViewBag.desde;
    var hasta = ViewBag.hasta;
    var time = DateTime.Now.ToString("HHmmss");
}

<div class="hpanel">
    <div class="panel-heading">
        <div class="panel-tools">
            <a class="showhide"><i class="fa fa-chevron-up"></i></a>
        </div>
        <i class="pe pe-7s-edit"></i> Listado de Solicitudes
    </div>
    <div class="panel-body">
        <p><button class="btn btn-xs btn-info" ><i class="fa fa-file-excel-o"></i> Exportar Excel</button></p>
        <div class="table-responsive" style="zoom:80%;">
            <table id="tbl_solicitud" class="table-striped table table-bordered dtbl" cellspacing="0">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>col1</th>
                        <th>col2</th>
                        <th>col3</th>
                        <th>col4</th>
                        <th>col5</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1</td>
                        <td>data1</td>
                        <td>data2</td>
                        <td>data3</td>
                        <td>data4</td>
                        <td>data5</td>
                    </tr>
                    <tr>
                        <td>2</td>
                        <td>data1</td>
                        <td>data2</td>
                        <td>data3</td>
                        <td>data4</td>
                        <td><a class="pe pe-7s-graph1 btn btn-success btn-xs" title="Ver Movimientos"></a></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
@section Styles {
    @Styles.Render("~/bundles/datatables/css")
}

@section Scripts {
    @Scripts.Render("~/bundles/datatables/js")
    @Scripts.Render("~/bundles/datatablesBootstrap/js")
<script src="~/Scripts/funciones.js?@time"></script>


    <script type="text/javascript">
        $(function () {
            $('#tbl_solicitud').dataTable();
        });
    </script>
}